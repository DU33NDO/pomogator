generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Assignment {
  id          String   @id @default(uuid()) 
  file        String   
  teacherName String   
  groupName   String   
  description String?
  deadline    DateTime 
  createdAt   DateTime @default(now()) 
}
